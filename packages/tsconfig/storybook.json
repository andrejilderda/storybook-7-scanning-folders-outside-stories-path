{
  "extends": "./base.json",
  "display": "Storybook",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": false,
    "jsx": "react-jsx",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "ESNext",
    "useDefineForClassFields": true
  },
  "include": ["src"],
  "references": [{ "path": "./storybook.node.json" }]
}
