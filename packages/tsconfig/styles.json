{
  "extends": "./base.json",
  "display": "Styles",
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "target": "ESNext",
    "useDefineForClassFields": true
  },
  "include": ["src"]
}
